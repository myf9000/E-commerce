          div class="tab-pane fade" id="sent"
            table class="table table-striped table-hover"
              h4 Sent orders
              thead
                tr class="success"
                  td
                    | Nr.
                  td
                    | Item
                  td
                    | Total Price
                  td
                    | Action

              tbody
              - @user.orders.product.each do |f|
                - f.each do |ff|
                  tr
                    - if ff.status == "sent"
                  
                      td 
                        span class="label label-warning"
                          div class="yolo"
                            = link_to ff.id, order_path(f) 
                      td
                        = ff.order_items.count
                      td
                        
                        = print_price ff.total
                      td
                        = link_to "Delivered", product_orders_order_item_path(OrderItem.find_by_order_id(ff.id), type: "Delivered")
                            
          
          div class="tab-pane fade" id="delivered"
            table class="table table-striped table-hover"
              h4 Delivered orders
              thead
                tr class="success"
                  td
                    | Nr.
                  td
                    | Item
                  td
                    | Total Price
                  td
                    | Action

              tbody
