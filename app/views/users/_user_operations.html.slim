div class="row"
  div class="col-md-2"
    ul class="nav nav-pills nav-stacked"
      li class="active"
        a href="#active_orders" data-toggle="tab" aria-expanded="false"  Active
      li 
        a href="#past" data-toggle="tab" aria-expanded="false" Past
      li 
        a href="#paid" data-toggle="tab" aria-expanded="false"  Paid
      li 
        a href="#sent" data-toggle="tab" aria-expanded="false" Sent
      li 
        a href="#delivered" data-toggle="tab" aria-expanded="false" Delivered

  div class="col-md-10"
    div class="panel panel-dafault"
      div class="panel-heading"
        div id="myTabContent" class="tab-content"
          div class="tab-pane fade active in" id="active_orders"
            table class="table table-striped table-hover"
              h4 Active orders
              thead
                tr class="success"
                  td
                    | Nr.
                  td
                    | Item
                  td
                    | Total Price
                  td
                    | Action

              tbody
              - @user.orders.product.each do |f|
                - f.each do |ff|
            
                  - if ff.status == "unsubmitted"  and ff.total > 0
                    tr
                      td 
                        span class="label label-warning"
                          div class="yolo"
                            = link_to ff.id, order_path(f) 
                      td
                        = ff.order_items.count
                      td
                        
                        = print_price ff.total
                      td
                        span class="label label-danger"
                          div class="yolo"
                            = ff.status


          div class="tab-pane fade" id="past"
            table class="table table-striped table-hover"
              h4 Past orders
              thead
                tr class="success"
                  td
                    | Nr.
                  td
                    | Item
                  td
                    | Total Price
                  td
                    | Action

              tbody
              - @user.orders.product.each do |f|
                - f.each do |ff|
                  tr
                    - if ff.status == "submitted"
                  
                      td 
                        span class="label label-warning"
                          div class="yolo"
                            = link_to ff.id, order_path(f) 
                      td
                        = ff.order_items.count
                      td
                        
                        = print_price ff.total
                      td
                        span class="label label-danger"
                          div class="yolo"
                            = ff.status

          div class="tab-pane fade" id="paid"
